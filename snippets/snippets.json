{
  "fprintf": {
    "prefix": "fprintf",
    "body": [
      "fprintf('$1');"
    ],
    "description": "fprintf"
  },
  "nargin": {
    "prefix": "nargin",
    "body": [
      "nargin"
    ],
    "description": "nargin"
  },
  "clc clear close all": {
    "prefix": "clc",
    "body": [
      "clc; clear; close all;",
      "",
      ""
    ],
    "description": "clc; clear; close all;"
  },
  "syms": {
    "prefix": "syms",
    "body": [
      "syms ${1:x}; % be careful performance issue.",
      "",
      "$0"
    ],
    "description": "syms"
  },
  "simplify": {
    "prefix": "simplify",
    "body": [
      "simplify($1);"
    ],
    "description": "simplify"
  },
  "- for": {
    "prefix": "for",
    "body": [
      "for ${1:index} = ${2:1} : ${3:n}",
      "\t$0",
      "end"
    ],
    "description": "for"
  },
  "axis": {
    "prefix": "axis",
    "body": [
      "axis([${1:xmin} ${2:xmax} ${3:ymin} ${4:ymax}]);"
    ],
    "description": "axis"
  },
  "fopen": {
    "prefix": "fopen",
    "body": [
      "${3:fileID} = fopen(${1:filename}, '${2:r}');"
    ],
    "description": "fopen"
  },
  "fscanf": {
    "prefix": "fscanf",
    "body": [
      "fscanf(${1:fileID}, '${2:%f %f}', [${3:2} Inf]).';"
    ],
    "description": "fscanf"
  },
  "length": {
    "prefix": "length",
    "body": [
      "length($1)"
    ],
    "description": "length"
  },
  "figure": {
    "prefix": "figure",
    "body": [
      "figure;"
    ],
    "description": "figure"
  },
  "plot": {
    "prefix": "plot",
    "body": [
      "plot(${1:x}, ${2:y});"
    ],
    "description": "plot"
  },
  "hold": {
    "prefix": "hold",
    "body": [
      "hold on;"
    ],
    "description": "hold"
  },
  "legend": {
    "prefix": "legend",
    "body": [
      "legend('$1', '$2'$3, 'Location', '${4:northeast}');"
    ],
    "description": "legend"
  },
  "- xlabel": {
    "prefix": "xlabel",
    "body": [
      "xlabel('$1');"
    ],
    "description": "xlabel"
  },
  "- ylabel": {
    "prefix": "ylabel",
    "body": [
      "ylabel('$1');"
    ],
    "description": "ylabel"
  },
  "- title": {
    "prefix": "title",
    "body": [
      "title('$1');"
    ],
    "description": "title"
  },
  "grid": {
    "prefix": "grid",
    "body": [
      "grid on;"
    ],
    "description": "grid"
  },
  "zeros": {
    "prefix": "zeros",
    "body": [
      "zeros($1);"
    ],
    "description": "zeros"
  },
  "ones": {
    "prefix": "ones",
    "body": [
      "ones($1);"
    ],
    "description": "ones"
  },
  "abs": {
    "prefix": "abs",
    "body": [
      "abs($1)"
    ],
    "description": "abs"
  },
  "diff": {
    "prefix": "diff",
    "body": [
      "diff($1);"
    ],
    "description": "diff"
  },
  "sin": {
    "prefix": "sin",
    "body": [
      "sin($1)"
    ],
    "description": "sin"
  },
  "cos": {
    "prefix": "cos",
    "body": [
      "cos($1)"
    ],
    "description": "cos"
  },
  "fgetl": {
    "prefix": "fgetl",
    "body": [
      "fgetl(${1:fileID});"
    ],
    "description": "fgetl"
  },
  "num2str": {
    "prefix": "num2str",
    "body": [
      "num2str($1)"
    ],
    "description": "num2str"
  },
  "fclose": {
    "prefix": "fclose",
    "body": [
      "fclose(${1:fileID});"
    ],
    "description": "fclose"
  },
  "double": {
    "prefix": "double",
    "body": [
      "double($1);"
    ],
    "description": "double"
  },
  "- function": {
    "prefix": "function",
    "body": [
      "function ${3:output} = ${1:my_fun}(${2:input})",
      "%",
      "% ${4:description}.",
      "%",
      "% @since ${5:1.0.0}",
      "% @param {${6:type}} [${7:name}] ${8:description}.",
      "% @return {${9:type}} [${10:name}] ${11:description}.",
      "% @see ${12:dependencies}",
      "%",
      "",
      "\t$0",
      "",
      "end",
      ""
    ],
    "description": "function"
  },
  "plot figure template": {
    "prefix": "pf",
    "body": [
      "figure;",
      "plot(${1:x}, ${2:y});",
      "title('$3');",
      "xlabel('$4');",
      "ylabel('$5');"
    ],
    "description": "plot figure template"
  },
  "size": {
    "prefix": "size",
    "body": [
      "size(${1:A}, ${2:dim})"
    ],
    "description": "size"
  },
  "sqrt": {
    "prefix": "sqrt",
    "body": [
      "sqrt($1)"
    ],
    "description": "sqrt"
  },
  "linspace": {
    "prefix": "linspace",
    "body": [
      "linspace($1);"
    ],
    "description": "linspace"
  },
}
