{
  "fprintf": {
    "prefix": "print",
    "body": [
      "fprintf('$1');"
    ],
    "description": "fprintf"
  },
  "nargin": {
    "prefix": "nargin",
    "body": [
      "nargin"
    ],
    "description": "nargin"
  },
  "clc clear close all": {
    "prefix": "clc",
    "body": [
      "clc; clear; close all;",
      "",
      ""
    ],
    "description": "clc; clear; close all;"
  },
  "syms": {
    "prefix": "syms",
    "body": [
      "syms ${1:x};"
    ],
    "description": "syms"
  },
  "simplify": {
    "prefix": "simplify",
    "body": [
      "simplify($1);"
    ],
    "description": "simplify"
  },
  "for": {
    "prefix": "for",
    "body": [
      "for ${1:index} = ${2:1} : ${3:n}",
      "\t$0",
      "end"
    ],
    "description": "for"
  },
  "axis": {
    "prefix": "axis",
    "body": [
      "axis([${1:xmin} ${2:xmax} ${3:ymin} ${4:ymax}]);"
    ],
    "description": "axis"
  },
  "fopen": {
    "prefix": "fopen",
    "body": [
      "${3:fileID} = fopen(${1:filename}, '${2:r}');"
    ],
    "description": "fopen"
  },
  "fscanf": {
    "prefix": "fscanf",
    "body": [
      "${4:A} = fscanf(${1:fileID}, '${2:%f %f}', [${3:2} Inf]).';"
    ],
    "description": "fscanf"
  },
  "length": {
    "prefix": "length",
    "body": [
      "length($1);"
    ],
    "description": "length"
  },
  "figure": {
    "prefix": "figure",
    "body": [
      "figure;"
    ],
    "description": "figure"
  },
  "plot": {
    "prefix": "plot",
    "body": [
      "plot(${1:x}, ${2:y});"
    ],
    "description": "plot"
  },
  "hold": {
    "prefix": "hold",
    "body": [
      "hold on;"
    ],
    "description": "hold"
  },
  "legend": {
    "prefix": "legend",
    "body": [
      "legend({'${1:}'}, 'Location', '${2:northeast}');"
    ],
    "description": "legend"
  },
  "xlabel": {
    "prefix": "xlabel",
    "body": [
      "xlabel('$1');"
    ],
    "description": "xlabel"
  },
  "ylabel": {
    "prefix": "ylabel",
    "body": [
      "ylabel('$1');"
    ],
    "description": "ylabel"
  },
  "title": {
    "prefix": "title",
    "body": [
      "title('$1');"
    ],
    "description": "title"
  },
  "grid": {
    "prefix": "grid",
    "body": [
      "grid on;"
    ],
    "description": "grid"
  },
  "zeros": {
    "prefix": "zeros",
    "body": [
      "zeros($1);"
    ],
    "description": "zeros"
  },
  "ones": {
    "prefix": "ones",
    "body": [
      "ones($1);"
    ],
    "description": "ones"
  },
  "abs": {
    "prefix": "abs",
    "body": [
      "abs($1);"
    ],
    "description": "abs"
  },
  "diff": {
    "prefix": "diff",
    "body": [
      "diff($1);"
    ],
    "description": "diff"
  },
}
